{
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.associations": {
    "*.svg": "html"
  },
  "[markdown]": {
    "editor.fontSize": 14,
    "editor.lineHeight": 18,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 64,
    "editor.lineNumbers": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.minimap.enabled": false
  }
}
